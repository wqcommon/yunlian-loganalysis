<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yunlian.loganalysis.dao.StatCallDailyApiDao">

    <insert id="insert" parameterType="com.yunlian.loganalysis.po.TbDemoPo">
        INSERT INTO tb_demo(address,phone)VALUES
        (
        #{address},
        #{phone}
        )
        <selectKey keyProperty="id" resultType="int" order="AFTER">
            SELECT LAST_INSERT_ID()
        </selectKey>
    </insert>

</mapper>